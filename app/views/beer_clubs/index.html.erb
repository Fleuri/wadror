<h1>Listing Beer Clubs</h1>

<p id="notice"><%= notice %></p>

<table>
  <tr>
    <th>Name</th>
    <th>Founded</th>
    <th>City</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @beer_clubs.each do |beer_club| %>
  <tr>
    <td><%= link_to beer_club.name, beer_club %></td>
    <td><%= beer_club.founded %></td>
    <td><%= beer_club.city %></td>
    <td><%= link_to 'Show', beer_club %></td>
    <td><%= link_to 'Edit', edit_beer_club_path(beer_club) %></td>
    <td><%= button_to 'Join', join_path(beer_club)%>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Beer club', new_beer_club_path %>
